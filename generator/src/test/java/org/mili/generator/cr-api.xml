<model package="jcrapi.model">
    <class name="Alliance">
        <member name="types" type="List(String)"/>
        <member name="roles" type="List(String)"/>
    </class>
    <class name="Arena">
        <member name="arenaID" type="int"/>
        <member name="imageURL" type="String"/>
        <member name="name" type="String"/>
        <member name="arena" type="String"/>
        <member name="trophyLimit" type="int"/>
    </class>
    <class name="ArenasItem">
        <member name="arenaID" type="int"/>
        <member name="name" type="String"/>
        <member name="arena" type="String"/>
        <member name="trophyLimit" type="int"/>
    </class>
    <class name="Badge">
        <member name="filename" type="String"/>
        <member name="key" type="String"/>
        <member name="url" type="String"/>
    </class>
    <class name="Badges">
    </class>
    <class name="CardsItem">
        <member name="decklink" type="String"/>
        <member name="elixir" type="int"/>
        <member name="cardKey" type="String"/>
        <member name="name" type="String"/>
        <member name="description" type="String"/>
        <member name="arena" type="int"/>
        <member name="type" type="String"/>
        <member name="card_id" alias="cardId" type="int"/>
        <member name="key" type="String"/>
        <member name="rarity" type="String"/>
    </class>
    <class name="ChestCycle">
        <member name="superMagicalPos" type="int"/>
        <member name="legendaryPos" type="int"/>
        <member name="epicPos" type="int"/>
        <member name="position" type="int"/>
    </class>
</model>